# ========== Frontend Environment Variables ==========

# ========== Backend API ==========
# Render-hosted backend service
VITE_API_URL=https://agroconnect-backend.onrender.com

# ========== Firebase Configuration ==========
# Your Firebase project configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# ========== ML Model Service ==========
# For direct ML model API calls (optional)
VITE_ML_API_URL=https://agroconnect-ml-model.onrender.com

# ========== Feature Flags ==========
VITE_ENABLE_GEMINI=true
VITE_ENABLE_OPENAI=true
VITE_ENABLE_CROP_ANALYSIS=true

# ========== App Configuration ==========
VITE_APP_NAME=AgroConnect
VITE_APP_VERSION=1.0.0
