# ML Model Service Environment Variables

# Port (Render will set this automatically)
PORT=5000

# Flask Environment
FLASK_ENV=production

# Model Configuration
MODEL_PATH=./src/crop_disease_model.h5
IMG_SIZE=224
MAX_IMAGE_SIZE=5242880

# Performance & Timeout Settings
REQUEST_TIMEOUT=30
INFERENCE_TIMEOUT=30

# Logging
LOG_LEVEL=INFO

# CORS Settings
CORS_ORIGINS=*

# Health Check
HEALTH_CHECK_INTERVAL=60
